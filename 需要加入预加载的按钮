    game.vars_.bossGoodsData=bossAwardVictoryData[i];\n        qyengine.instance_create(0,0,\"guildBossGood\",{\"type\":\"guildBossGood\",\"id\":'item'+i,\"zIndex\":0});\n        wordBossVictoryRoll.appendChild(\"item\"+i,0,0,0,i,false,true);\n    }\n    qyengine.guardId(\"wordBossVictoryPanel_txt_7\").setText(\"\");\n }\n}\n\n\n\n\n//伤害排名\nqyengine.guardId(\"wordBossVictoryPanel_txt\").setText(\"伤害排名:  \"+hurtRankingNum);\n//累计伤害\nqyengine.guardId(\"wordBossVictoryPanel_txt_1\").setText(\"累计伤害达到:  \"+hurtNum);\n//伤害第一名\nqyengine.guardId(\"wordBossVictoryPanel_txt_2\").setText(\"伤害第一名:  \"+hurtFristName);\n//最后一击\nqyengine.guardId(\"wordBossVictoryPanel_txt_3\").setText(\"最后一击:  \"+hurtLastName);\n//时间\nqyengine.guardId(\"wordBossVictoryPanel_txt_4\").setText(\"击杀耗时:  \"+window.get_time_text(hurtTime));\n//伤害奖励\n\nqyengine.guardId(\"wordBossVictoryPanel_txt_5\").setText(\"伤害奖励银子:  \"+serNum);\nvar currentNum = 5;\n\nself.vars_.timeOutHander = window.setInterval(function(){\n    \n    currentNum--;\n    \n    if(currentNum == 0){\n        \n        window.clearInterval(self.vars_.timeOutHander);\n\nif(current_scene.vars_.currentWavesType <2)\n{\n    if(qyengine.getInstancesByType(\"grou_battle\").length > 0){\n\t\n\tif(current_scene.vars_.heroObj && (current_scene.vars_.heroObj.vars_.BOSSFIGHTING ||       current_scene.vars_.heroObj.vars_.GUILDBOSSFIGHTING)){\n\t\twindow.clearInterval(self.vars_.timeOutHander);\n\t\t//此处添加删除胜利或失败的组合UI\n\t\tqyengine.guardId(\"wordBossVictoryPanel\").destroy();\n\t\tgame.scripts[\"al_scr_main_sceneBattleInfoInit\"](null,null);\n\t\t\n\t\treturn;\n\t}\n}\nwindow.clearInterval(self.vars_.timeOutHander);\n  qyengine.guardId(\"wordBossVictoryPanel\").destroy();\n}else\n{\nif(game.vars_.wordBossVictoryDataTitile==1)\n{\nKBEngine.app.player().baseCall('reqOutBossBattle');\n}else if(game.vars_.wordBossVictoryDataTitile==0)\n{\n  KBEngine.app.player().baseCall('reqLevelGuildBossScene',game.vars_.guildBossId);\n}\n\ngame.scripts[\"al_scr_\" + \