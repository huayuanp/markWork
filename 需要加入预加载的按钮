BossLv);\n//Boss名字\nqyengine.guardId(\"worldBossText_6\").setText(\"世界Boss\");\n\nvar  bossLv=game.vars_.worldBossLv;\nvar  bossName=getConfig('monster',19901,'name');\nvar bossBloodData=getConfig('monster',19901,'hp').split(\"|\");\n\nvar  bloodNum=Math.floor(bossBloodData[0]*Math.pow(bossBloodData[1]-bossBloodData[2]*bossLv,bossLv)/100000000);\n\n//设置血条\nqyengine.guardId(\"worldBossPanelDidNotOpen_Blood\").currentSprite.width=qyengine.guardId(\"worldBossPanelDidNotOpen_Blood\").currentWidth*((curryBossBlood/100000000)/bloodNum);\n\n//Boss血量\nqyengine.guardId(\"worldBossPanelDidNotOpenBloodNum\").setText(Math.floor(curryBossBlood/100000000)+\"亿/\"+bloodNum+\"亿\");\n\nif(game.vars_.worldFrightAutomatic==true&&game.vars_.bossWarAutoId==19901)\n{\n  qyengine.guardId(\"worldBossPanelDidNotOpen_Pigeon\").setHidden(false);\n}else\n{\n  qyengine.guardId(\"worldBossPanelDidNotOpen_Pigeon\").setHidden(true);\n}\n\n \nif(game.vars_.bossFightCdNum<=0||game.vars_.bossFightCdNum==null)\n{\n  qyengine.guardId(\"worldBossCdTimeTex\").setText(\"\");\n  qyengine.guardId(\"obj_BOSS战_箭头\").setHidden(true);\n  \n}else\n{\n  qyengine.guardId(\"worldBossCdTimeTex\").setText(\"冷却时间:\"+game.vars_.bossFightCdNum+\"秒\");\n  qyengine.guardId(\"obj_BOSS战_箭头\").setHidden(false);\n}",
